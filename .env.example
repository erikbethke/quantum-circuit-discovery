# IonQ API Configuration
IONQ_API_KEY=your_api_key_here
IONQ_API_URL=https://api.ionq.co
IONQ_API_VERSION=v0.4

# OpenAI Configuration (for circuit classification)
OPENAI_API_KEY=your_openai_key_here

# Anthropic Configuration (optional, for circuit classification) 
ANTHROPIC_API_KEY=your_anthropic_key_here

# Database Configuration (optional, for production)
DATABASE_URL=postgresql://user:password@localhost/dfal

# Redis Configuration (optional, for caching)
REDIS_URL=redis://localhost:6379

# Monitoring (optional)
WANDB_API_KEY=your_wandb_key_here
WANDB_PROJECT=quantum-discovery

# Evolution Settings
DEFAULT_POPULATION_SIZE=100
DEFAULT_GENERATIONS=50
DEFAULT_MUTATION_RATE=0.3
DEFAULT_CROSSOVER_RATE=0.7

# Hardware Settings
USE_HARDWARE=false
DEFAULT_BACKEND=simulator
DEFAULT_SHOTS=1000

# Output Settings
OUTPUT_DIR=discoveries
SAVE_CHECKPOINTS=true
CHECKPOINT_FREQUENCY=5